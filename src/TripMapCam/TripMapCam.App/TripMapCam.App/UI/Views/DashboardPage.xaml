<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="TripMapCam.App.UI.Views.DashboardPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:extensions="clr-namespace:TripMapCam.App.UI.Extensions"
    xmlns:views="clr-namespace:TripMapCam.App.UI.Views"
    xmlns:vm="clr-namespace:TripMapCam.App.UI.ViewModels"
    extensions:VisibilityChangeExtension.NotifyVisibilityChanged="True"
    x:DataType="vm:DashboardViewModel">
    <ContentPage.BindingContext>
        <vm:DashboardViewModel />
    </ContentPage.BindingContext>
    <views:PreviewView BindingContext="{Binding PreviewViewModel}" />
</ContentPage>
